// Fibonacci.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"
#define MAX 100

using namespace std;

void cargar ( int v[], int tam );
void mostrar ( int v[], int tam );

void main ()
{
	int v[MAX], tam;
	cout<<"Ingrese el numero fiboanacci: "<<endl;
	cin>>tam;
	cargar ( v, tam );
	mostrar ( v, tam );

	getch();

}
void cargar ( int v[], int tam )
{
	v[0]=0;
	v[1]=1;
	for ( int i=2; i<tam; i++)
		v[i]=v[i-1]+v[i-2];
}
void mostrar ( int v[], int tam )
{
	for ( int i=0; i<tam; i++ )
		cout<<v[i];
}

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main() {
	int n, i = 1, num = 0, sum = 0;
	cout<<"Ingrese el numero n de terminos: ";
	cin>>n;

	while (i<=n) {
		cout<<"digite el numero: ";
		cin>>num;
		sum = sum+num;
		i++;

	}
	cout<<"El numero es: "<<sum<<endl;

getch();
}
